@model IEnumerable<CRM.Models.VTotalEjecucionesConCierre>

@{
    ViewData["Title"] = "Index";
}

<h1>Total de ejecuciones con cierre</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Total)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
        </tr>
}
    </tbody>
</table>
